{
  "expo": {
    "name": "vibeWave",
    "slug": "vibeWave",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/vwsplash.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "splash": {
        "image": "./assets/images/vwicon.png",
        "backgroundColor": "#fffffff",
        "resizeMode": "contain"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/vwicon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["FOREGROUND_SERVICE", "WAKE_LOCK"],
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/vwicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/vwicon.png",
          "backgroundColor": "#fffffff",
          "resizeMode": "contain"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
